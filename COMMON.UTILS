
PROGRAM COMMON.UTILS
* Common Utilities (bloated intentionally)

SUBROUTINE LOG(MSG)
   CRT DATE(): " - ": MSG
RETURN

SUBROUTINE VALIDATE.NOT.EMPTY(VALUE, ERR)
   IF VALUE = "" THEN
      CRT ERR
      STOP
   END
RETURN

SUBROUTINE CALC.TAX(AMOUNT, RATE, RESULT)
   RESULT = AMOUNT * RATE
RETURN
END
